<!DOCTYPE html>
<html>

<%- include('./partials/head.ejs')  %>

<body>
    <div class='container-fluid'>
        <div class='row'>
            <%- include('./partials/sidebar.ejs') %>
            <div class='col-sm-10'>
                <h1><%= title %> </h1>
                <ul>
                    <% if (author_list) { %>


                    <% author_list.forEach(author => { %>
                    <li>
                        <a href=<%= author.url %>> <%= author.name %>
                            <%= author.lifespan %> </a>
                    </li>
                    <% }) %>
                    <% } else { %>
                    <li>
                        There are no authors
                    </li>

                    <% } %>
                </ul>
            </div>
        </div>
    </div>

</body>

</html>